{
  "compilerOptions": {
    "strict": true,
    "outDir": "./dist/",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "declaration": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
